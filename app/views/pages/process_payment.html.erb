<div class="singleheader">
            <div class="container">
                <div class="col-lg-6 col-sm-6 col-md-6">
                    <div class="single-title">
                        <h3>Process Subscription Payment</h3>
                        <h4>Pay Your Subscription With Paypal</h4>
                    </div>
                </div>
                <div class="col-lg-6 col-sm-6 col-md-6">
                    <div class="breadcrumb-container">
                        <ul class="breadcrumb">
                            <li class="active">Our offers are very affordable!</li>
                        </ul>
                    </div>
                </div>
            </div>  
            <div class="shadow"></div>
        </div>
<div id="content" class="single col-lg-12 col-md-12 col-sm-12">
			
            <div class="col-lg-12 col-md-12 col-sm-12">

                 <div class="alert alert-warning">
                                                    <span class="label label-warning">Your in the last step now! </span>
                                                    <span>Click the Paypal Checkout Button Finish The Process!</span>
                                                </div>

                        <div class="col-lg-4 col-md-4 col-sm-12">
                            <div class="dm-pricing-table">
                                
                                </div>
                                </div>

                        <% if @profile.subscription_type == "Standard" %>
                        <div class="col-lg-4 col-md-4 col-sm-12">
                            <div class="dm-pricing-table">
                                <div class="dm-pricing">
                                
                                    <div class="dm-pricing-header">
                                        <h5>Standard</h5>
                                        <div class="dm-pricing-cost">£3</div>
                                        <div class="dm-pricing-per">per month</div>
                                    </div>
                                    
                                    <div class="dm-pricing-content">
                                        <ul>
                                            <li>5 Contracts</li>
                                            <li>30 Reminders / Emails (Days)</li>
                                            <li>30 Mobile App Link (Days)</li>
                                            <li>Dashboard</li>
                                            <li>Unlimited Tenants</li>
                                            <li>Unlimited Properties</li>
                                            <li>Unlimited Tenant History</li>
                                            <li>Unlimited Property History</li>
                                        </ul>
                                    </div>
                                    
                                    <div class="dm-pricing-button">
                                        <%= link_to image_tag("https://www.paypal.com/en_US/i/btn/btn_xpressCheckout.gif"), @sub.paypal_url(root_url + 'subscription_payment/'+ @sub.return_token + '/confirm') 
                                        %>
                                   </div>
                                   
                                </div>
                            </div>
                        </div>
                        <% end %>
                        
                        <% if @profile.subscription_type == "Pro" %>
                        <div class="col-lg-4 col-md-4 col-sm-12">
                            <div class="dm-pricing-table">
                                <div class="dm-pricing">
                                
                                    <div class="dm-pricing-header">
                                        <h5>Pro</h5>
                                        <div class="dm-pricing-cost">£5</div>
                                        <div class="dm-pricing-per">per month</div>
                                    </div>
                                    
                                    <div class="dm-pricing-content">
                                        <ul>
                                            <li>6-10 Contracts</li>
                                            <li>30 Reminders / Emails (Days)</li>
                                            <li>30 Mobile App Link (Days)</li>
                                            <li>Dashboard</li>
                                            <li>Unlimited Tenants</li>
                                            <li>Unlimited Properties</li>
                                            <li>Unlimited Tenant History</li>
                                            <li>Unlimited Property History</li>
                                        </ul>
                                    </div>
                                    
                                    <div class="dm-pricing-button">
                                        <%= link_to image_tag("https://www.paypal.com/en_US/i/btn/btn_xpressCheckout.gif"), @sub.paypal_url(root_url + 'subscription_payment/'+ @sub.return_token + '/confirm') 
                                        %>

                                   </div>
                                   
                                </div>
                            </div>
                        </div>
                        <% end %>
                        
                        <% if @profile.subscription_type == "Enterprise" %>

                        <div class="col-lg-4 col-md-4 col-sm-12">
                            <div class="dm-pricing-table">
                                <div class="dm-pricing">
                                
                                    <div class="dm-pricing-header green">
                                        <h5>Enterprise</h5>
                                        <div class="dm-pricing-cost">£10</div>
                                        <div class="dm-pricing-per">per month</div>
                                    </div>
                                    
                                    <div class="dm-pricing-content">
                                        <ul>
                                            <li>11-unlimited Contracts</li>
                                            <li>30 Reminders / Emails (Days)</li>
                                            <li>30 Mobile App Link (Days)</li>
                                            <li>Dashboard</li>
                                            <li>Unlimited Tenants</li>
                                            <li>Unlimited Properties</li>
                                            <li>Unlimited Tenant History</li>
                                            <li>Unlimited Property History</li>
                                        </ul>
                                    </div>
                                    
                                    <div class="dm-pricing-button">
                                        <%= link_to image_tag("https://www.paypal.com/en_US/i/btn/btn_xpressCheckout.gif"), @sub.paypal_url(root_url + 'subscription_payment/'+ @sub.return_token + '/confirm') 
                                        %>
                                   </div>
                                   
                                </div>
                            </div>
                        </div>
                        <% end %>
                        <div class="col-lg-4 col-md-4 col-sm-12">
                            <div class="dm-pricing-table">
                                
                                </div>
                                </div>
						
                        
			</div>
            
            </div>