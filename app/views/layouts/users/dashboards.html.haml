    .main-container
      = render 'layouts/users/dashboard_left_navigation'
      / start: PAGE
      / start: PAGE CONTENT
      .main-content
        / start: PANEL CONFIGURATION MODAL FORM
        #panel-config.modal.fade{"aria-hidden" => "true", role: "dialog", tabindex: "-1"}
          .modal-dialog
            .modal-content
              .modal-header
                %button.close{"aria-hidden" => "true", "data-dismiss" => "modal", type: "button"}
                  \×
                %h4.modal-title Panel Configuration
              .modal-body
                Here will be a configuration form
              .modal-footer
                %button.btn.btn-default{"data-dismiss" => "modal", type: "button"}
                  Close
                %button.btn.btn-primary{type: "button"}
                  Save changes
            / /.modal-content
          / /.modal-dialog
        / /.modal
        / end: SPANEL CONFIGURATION MODAL FORM
        .container
          / start: PAGE HEADER
          .row
            .col-sm-12
              / start: PAGE TITLE & BREADCRUMB
              %ol.breadcrumb
                %li
                  %i.clip-home-3
                  %a{href: "#"}
                    Home
                %li.active
                  Dashboard
                %li.search-box
                  %form.sidebar-search
                    .form-group
                      %input{placeholder: "Start Searching...", type: "text"}/
                      %button.submit
                        %i.clip-search-3
              .page-header
                %h1
                  Dashboard
                  %small overview & stats
              / end: PAGE TITLE & BREADCRUMB
          / end: PAGE HEADER
          = yield
          / end: PAGE
          / end: PAGE CONTENT
