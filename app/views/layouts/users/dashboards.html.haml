    #event-management.modal.fade{"data-width" => "760", style: "display: none;", tabindex: "-1"}
      .modal-dialog
        .modal-content
          .modal-header
            %button.close{"aria-hidden" => "true", "data-dismiss" => "modal", type: "button"}
              \×
            %h4.modal-title Event Management
          .modal-body
          .modal-footer
            %button.btn.btn-light-grey{"data-dismiss" => "modal", type: "button"}
              Close
            %button.btn.btn-danger.remove-event.no-display{type: "button"}
              %i.fa.fa-trash-o
              Delete Event
            %button.btn.btn-success.save-event{type: "submit"}
              %i.fa.fa-check
              Save
